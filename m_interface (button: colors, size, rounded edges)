					EvalSound('audioAng');
				}
			}

			function clearContents(txtArea)
			{
				if(bodyTextClickCtr==0)
				{
					txtArea.value='';
					bodyTextClickCtr+=1;
				}
				else
					return;
			}
			function resetText()
			{
				document.getElementById('diary').value='...';
				bodyTextClickCtr=0;
			}
