		<script>
			var bodyTextClickCtr=0;
			function pushed(a)
			{
				if(a=='Serene')
					{
					document.getElementById('img').src="http://nooneexistsalone.files.wordpress.com/2011/10/sunrise2.jpg";
					document.getElementById('img2').src="http://img.4plebs.org/boards/x/image/1391/86/1391864103957.jpg";
					document.getElementById('img3').src='http://beststockpictures.toinspire.in/plog-content/images/widescreen-wallpapers/nature-pictures-2/nature-pictures-single-tree-bright-clouds-naute-pictures.jpg';
					EvalSound('audioSer');
					}
				if(a=='Content')
				{
					document.getElementById('img').src="http://flowerdrawings.net/wp-content/uploads/2014/03/1095860966_a03c9cb69c.jpg";
					document.getElementById('img2').src="http://wallpapers.wallbase.cc/rozne/wallpaper-326813.jpg";
					document.getElementById('img3').src='http://wallpapers.wallbase.cc/rozne/wallpaper-68013.jpg';
					EvalSound('audioCon');
				}
				if(a=='Elated')
				{
					document.getElementById('img').src="http://www.healthyhappylife.org/images/happy-people.jpg";
					document.getElementById('img2').src="http://p1.pichost.me/i/30/1535145.jpg";
					document.getElementById('img3').src='http://3.bp.blogspot.com/-F1IBOG0J9WA/SRSQEL6nPPI/AAAAAAAAAFs/PbIda7t0HFc/s1600/Smiley+Flower+Happy%2521.jpg';
					EvalSound('audioEla');
				}
				if(a=='Contemplative')
				{
					document.getElementById('img').src="http://4loveofthetruth.files.wordpress.com/2012/01/contemplation21.jpg";
					document.getElementById('img2').src="https://z-n.ak.fbcdn.net/external.ak/safe_image.php?d=AQBRb2wW-6Hxi00p&w=376&h=366&url=http%3A%2F%2Fhollyroach.com%2Fwp-content%2Fuploads%2F2013%2F09%2Fmeditating-by-the-lake.jpg";
					document.getElementById('img3').src='http://upload.wikimedia.org/wikipedia/commons/e/e8/Water_drop_impact_on_a_water-surface.jpg';
					EvalSound('audioCmp');
				}
				if(a=='Neutral')
				{
					document.getElementById('img').src="http://upload.wikimedia.org/wikipedia/commons/c/ca/Imperial_sand_dunes.jpg";
					document.getElementById('img2').src="http://blog.gooddays-app.com/wp-content/uploads/2013/04/balance.jpg";
					document.getElementById('img3').src='http://lowcadence.com/wp-content/uploads/2013/08/YinYang.jpg';
					EvalSound('audioNeu');
				}
				if(a=='Energetic')
				{
					document.getElementById('img').src="http://fc00.deviantart.net/fs70/f/2010/056/1/f/Running_Fire_by_karimbalaa.jpg";
					document.getElementById('img2').src="http://img3.wikia.nocookie.net/__cb20130506161118/powerlisting/images/f/f6/Blue_energy-blog.jpg";
					document.getElementById('img3').src='http://lunar.thegamez.net/coalmine2014/images-how-coal-is-mined/net-3d-wallpaper-blue-energy-tornado-html-blue-energy-tornado-1920x1200.jpg';
					EvalSound('audioEne');
				}
				if(a=='Depressed')
				{
					document.getElementById('img').src="http://dionwu.com/img/framesoflife/isolate.jpg";
					document.getElementById('img2').src="http://i.imgur.com/ZBBukdp.jpg";
					document.getElementById('img3').src='http://photographyunposed.files.wordpress.com/2012/09/foggy-morning-web.jpg';
					EvalSound('audioDep');
				}
				if(a=='Confused')
				{
					document.getElementById('img').src="http://forbids.files.wordpress.com/2011/05/penrosestairs.png";
					document.getElementById('img2').src="http://undr.com/portfolio/desktops/UndrDesktop_BlurryShipLights.jpg";
					document.getElementById('img3').src='http://2.bp.blogspot.com/-k8K3WWi8Gvw/Tp3yyW9l1jI/AAAAAAAADBg/YWmnsG7wJn0/s1600/feet%2Bdancing.jpg';
					EvalSound('audioCfs');
				}
				if(a=='Angry')
				{
					document.getElementById('img').src="http://wallpapers.wallbase.cc/rozne/wallpaper-767625.jpg";
					document.getElementById('img2').src="http://wallpapers.wallbase.cc/rozne/wallpaper-2426010.jpg";
